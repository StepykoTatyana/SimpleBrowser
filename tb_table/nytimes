erTimer_metro = setInterval(function() {\n      var prev = counter;\n      counter++;\n      if(counter \u003E  images.length-1) counter = 0\n        images[counter].classList.add('g-home-fader-photo-active');\n        \u002F\u002F console.log(prev, counter)\n\n        setTimeout(function(){\n          \u002F\u002F console.log(prev, counter)\n         images[prev].classList.remove('g-home-fader-photo-active');\n        }, 250)\n    },  speed);\n\n    \u002F\u002F NYT app detection code\n    var isInApp = !!(\n      \u002F\u002F sometimes this query param is present\n      window.location.search.indexOf('nytapp') \u003E -1 ||\n      \u002F\u002F usually the user agent is set\n      window.navigator.userAgent.match(\u002Fnyt[-_]?(?:ios|android)\u002Fi) ||\n      \u002F\u002F on hybrid articles in android, the user agent and qs is missing\n      (window.navigator.userAgent.match(\u002Fandroid\u002Fi) && window.__HYBRID__)\n    );\n\n    var isInAppHome = document.querySelector('.nytapp-home-interactive') !== null;\n    var isAndroid = window.navigator.userAgent.indexOf('Android') \u003E -1;\n    var isIos = !!window.navigator.userAgent.match(\u002FiP(hone|ad)\u002Fi);\n\n    if (isInApp && isAndroid) document.querySelector('html').classList.add('nytapp-android');\n    if (isInApp && isIos) document.querySelector('html').classList.add('nytapp-ios');\n\n    \u002F\u002F don't resize if inside of programming list\n    if (!document.querySelector(\"#programming-list\")) {\n    \u002F\u002F NYT apps resize function\n    \u002F\u002F Resizes the interactive webview height after a 1 second pause\n      if ( window.NYTG && window.NYTG.enqueue) {\n        setTimeout(function() {\n          var container = document.querySelector(\"#metro-home-fader.g-home-fader\");\n          var windowWidth = window.innerWidth;\n          function resize() {\n            var height = container.offsetHeight;\n            window.NYTG.enqueue(\n              JSON.stringify({ id: 0, type: 'resize', options: { height: height } })\n              )\n          }\n          window.addEventListener(\"resize\", function() {\n            if ( window.innerWidth != windowWidth ) {\n              windowWidth = window.innerWidth;\n              resize();\n            }\n          });\n          resize();\n        }, 1000);\n      }\n    }\n\n  })();\n\n\u003C\u002Fscript\u003E\n","credit":"","slug":"home-fader-metro-promo","compatibility":"INLINE","storyFormat":null,"tone":"NEWS","uri":"nyt:\u002F\u002Fembeddedinteractive\u002Fe0c417c8-b90d-5135-a222-a4ea532788c0","url":"null"},"Section:U2VjdGlvbjpueXQ6Ly9zZWN0aW9uLzM5NDgwMzc0LTY2ZDMtNTYwMy05Y2UxLTU4Y2ZhMTI5ODhlMg==":{"id":"U2VjdGlvbjpueXQ6Ly9zZWN0aW9uLzM5NDgwMzc0LTY2ZDMtNTYwMy05Y2UxLTU4Y2ZhMTI5ODhlMg==","displayName":"New York","__typename":"Section"},"Image:SW1hZ2U6bnl0Oi8vaW1hZ2UvYmZhNWYyMzYtNDM0My01Nzc1LTkxODMtNjQyYjRkMWZlNjhi":{"id":"SW1hZ2U6bnl0Oi8vaW1hZ2UvYmZhNWYyMzYtNDM0My01Nzc1LTkxODMtNjQyYjRkMWZlNjhi","credit":"Amr Alfiky\u002FThe New York Times","crops({\"cropNames\":[\"MASTER\",\"MEDIUM_SQUARE\",\"THREE_BY_TWO\"],\"renditionNames\":[\"threeByTwoSmallAt2X\",\"threeByTwoMediumAt2X\",\"threeByTwoLargeAt2X\",\"square320\",\"articleLarge\",\"square640\",\"videoSixteenByNine310\",\"videoSixteenByNine500\",\"videoSixteenByNine1050\",\"videoSixteenByNine1920\"]})":[{"type":"id","generated":true,"id":"Image:SW1hZ2U6bnl0Oi8vaW1hZ2UvYmZhNWYyMzYtNDM0My01Nzc1LTkxODMtNjQyYjRkMWZlNjhi.crops({\"cropNames\":[\"MASTER\",\"MEDIUM_SQUARE\",\"THREE_BY_TWO\"],\"renditionNames\":[\"threeByTwoSmallAt2X\",\"threeByTwoMediumAt2X\",\"threeByTwoLargeAt2X\",\"square320\",\"articleLarge\",\"square640\",\"videoSixteenByNine310\",\"videoSixteenByNine500\",\"videoSixteenByNine1050\",\"videoSixteenByNine1920\"]}).0","typename":"ImageCrop"},{"type":"id","generated":true,"id":"Image:SW1hZ2U6bnl0Oi8vaW1hZ2UvYmZhNWYyMzYtNDM0My01Nzc1LTkxODMtNjQyYjRkMWZlNjhi.crops({\"cropNames\":[\"MASTER\",\"MEDIUM_SQUARE\",\"THREE_BY_TWO\"],\"renditionNames\":[\"