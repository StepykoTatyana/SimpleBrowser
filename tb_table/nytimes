					<script>
						Promise.all([
							NYTG.fetch("https://static01.nyt.com/newsgraphics/2020/03/16/coronavirus-maps/44be3d845ae3e56fe21a31fa2031c6c285f59701/data/timeseries/en/USA-only.json"), NYTG.fetch("https://static01.nyt.com/newsgraphics/2020/03/16/coronavirus-maps/44be3d845ae3e56fe21a31fa2031c6c285f59701/data/timeseries/en/NYT-World-only.json"), NYTG.fetch("https://static01.nyt.com/newsgraphics/2020/03/16/coronavirus-maps/44be3d845ae3e56fe21a31fa2031c6c285f59701/data/timeseries/en/CAN.json"), NYTG.fetch("https://static01.nyt.com/newsgraphics/2020/03/16/coronavirus-maps/44be3d845ae3e56fe21a31fa2031c6c285f59701/data/us_counties.json"), NYTG.fetch("https://static01.nyt.com/newsgraphics/2020/03/16/coronavirus-maps/44be3d845ae3e56fe21a31fa2031c6c285f59701/data/vaccine_summary.json")
						]).then(function(data) {
							var target = document.querySelector('[data-component="cv1"]');
							var url = NYTG.VIRUS_ASSETS + '/build/js/dashboard.js';
							var props = { timeseries: data[0], timeseriesWorld: data[1], timeseriesCanada: data[2], counties: data[3], vaccine_summary: data[4], average_change_cases_two_weeks: "19", average_change_deaths_two_weeks: "-31", average_change_cases_two_weeks_world: "23", average_change_deaths_two_weeks_world: "58", average_change_cases_two_weeks_can: "31", average_change_deaths_two_weeks_can: "-17", us_maps: ["usa_map","vaccine_distribution"], int_maps: ["world_map","vaccine_map","usa_map"], show_county: true, type: "top-stories" }

							
							let component;

							NYTG.load(url).then((mod) => {
								const Component = mod.default;

								component = new Component({
									target,
									hydrate: true,
									props
								});
							});
						
						});
					</script>
					
