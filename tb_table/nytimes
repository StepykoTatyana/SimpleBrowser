<script>
  if (!window.Promise || !window.fetch || !window.URL) {
    document.write('<script src="https://static01.nyt.com/newsgraphics/2020/03/16/coronavirus-maps/44be3d845ae3e56fe21a31fa2031c6c285f59701/polyfills.js"><\/script>');
